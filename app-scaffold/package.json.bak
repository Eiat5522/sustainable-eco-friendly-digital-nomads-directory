{
  "name": "app-scaffold",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "next": "14.2.28"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:performance": "node scripts/run-baseline-tests.js",
    "test:performance:staging": "node scripts/run-baseline-tests.js --env=staging",
    "test:performance:prod": "node scripts/run-baseline-tests.js --env=production",
    "analyze": "ANALYZE=true ANALYZE_OPEN=true next build"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "2.0.0",
    "@sanity/client": "^6.12.3",
    "@sanity/image-url": "^1.0.2",
