    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "framer-motion": "^10.18.0",
    "groq": "^3.23.4",
    "leaflet": "^1.9.4",
    "mongodb": "^6.3.0",
    "next": "14.2.28",
    "next-auth": "^4.24.5",
    "next-sanity": "^7.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.5.3",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/leaflet": "^1.9.8",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.3.2",
    "playwright": "^1.41.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^4.1.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
