{
  "servers": {
    "next-vercel": {
      "url": "https://gitmcp.io/vercel/next.js"
    },
    "next-saas-starter-Docs": {
      "url": "https://gitmcp.io/nextjs/saas-starter"
    },
    "ts-nextjs-tailwind-starter-Docs": {
      "url": "https://gitmcp.io/theodorusclarence/ts-nextjs-tailwind-starter"
    },
    "next-sanity-Docs": {
      "url": "https://gitmcp.io/sanity-io/next-sanity"
    },
    "next-sanity-studio-Docs": {
      "url": "https://gitmcp.io/sanity-io/next-sanity-studio"
    },
    "next-sanity-studio-graphql-Docs": {
      "url": "https://gitmcp.io/sanity-io/next-sanity-studio-graphql"
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "--allow-dirs",
        "${workspaceFolder}/app-next-directory",
        "--allow-dirs",
        "${workspaceFolder}/sanity"
      ]
    },
    "prysm-scraper": {
      "command": "npx",
      "args": ["-y", "@pinkpixel/prysm-mcp"],
      "env": {
        "PRYSM_OUTPUT_DIR": "${workspaceFolder}/docs/shared/PRYSM_OUTPUT/PRYSM_OUTPUT_DIR/",
        "PRYSM_IMAGE_OUTPUT_DIR": "${workspaceFolder}/docs/shared/PRYSM_OUTPUT/PRYSM_IMAGE_OUTPUT_DIR/"
      }
    }
  },
}
