{
  "servers": {
    "next-vercel": {
      "url": "https://gitmcp.io/vercel/next.js"
    },
    "next-saas-starter-Docs": {
      "url": "https://gitmcp.io/nextjs/saas-starter"
    },
    "ts-nextjs-tailwind-starter-Docs": {
      "url": "https://gitmcp.io/theodorusclarence/ts-nextjs-tailwind-starter"
    },
    "next-sanity-Docs": {
      "url": "https://gitmcp.io/sanity-io/next-sanity"
    },
    "next-sanity-studio-Docs": {
      "url": "https://gitmcp.io/sanity-io/next-sanity-studio"
    },
    "next-sanity-studio-graphql-Docs": {
      "url": "https://gitmcp.io/sanity-io/next-sanity-studio-graphql"
    },
    "workspace-filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${workspaceFolder}"
      ],
      "disabled": false,
      "autoApprove": [
        "list_allowed_directories"
      ]
    },
    "vscode-mcp-server": {
      "command": "npx",
      "args": ["mcp-remote@next", "http://localhost:3000/mcp"]
    },   
    "desktop-commander": {
      "timeout": 60,
      "command": "npx",
      "args": [
        "-y",
        "@wonderwhy-er/desktop-commander",
        "D:\\Eiat_Folder\\MyProjects\\MyOtherProjects\\sustainable-eco-friendly-digital-nomads-directory"
      ],
      "type": "stdio",
      "disabled": true
    },
    "chain-of-thought": {
      "command": "npx",
      "args": ["-y", "mcp-chain-of-thought"],
      "env": {
        "DATA_DIR": "${workspaceFolder}",
        "ENABLE_THOUGHT_CHAIN": "true",
        "TEMPLATES_USE": "en",
        "ENABLE_GUI": "true",
        "ENABLE_DETAILED_MODE": "true"
      }
  }
}
