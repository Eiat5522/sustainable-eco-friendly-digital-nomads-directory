{
  "explorer.excludeGitIgnore": false,
  "search.useParentIgnoreFiles": true,
  "search.useGlobalIgnoreFiles": true,
  "terminal.integrated.tabs.defaultColor": "terminal.ansiBlue",
  "terminal.integrated.cwd": "",
  "search.useIgnoreFiles": false,
  "markdownlint.lintWorkspaceGlobs": [
    "**/*.{md,mkd,mdwn,mdown,markdown,markdn,mdtxt,mdtext,workbook}",
    "!**/*.code-search",
    "!**/bower_components",
    "!**/node_modules",
    "!**/.git",
    "!**/vendor",
    "!**/*.instructions.md"
  ],
  "gitlens.graph.onlyFollowFirstParent": true,
  "git.openRepositoryInParentFolders": "never",
  "npm.packageManager": "pnpm",
  "jest.jestCommandLine": "pnpm --filter app-next-directory exec jest",
  "jest.enable": true,
  "jest.debugMode": false,
  "jest.runMode": "on-demand",
  "jest.rootPath": "app-next-directory",
  "jest.coverageFormatter": "GutterFormatter"
}
